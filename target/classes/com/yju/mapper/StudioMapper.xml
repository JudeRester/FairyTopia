<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yju.mapper.StudioMapper">
	<insert id="createWorkplace" useGeneratedKeys="true"
		keyProperty="workplace_id">
		insert into workplace(workplace_id,workplace_name, workplace_detail)
		values(wp_id.nextval,#{workplace_name},#{workplace_name})
	</insert>
	
	<insert id="addAuthor">
		insert into workplace_person values(#{workplace_id}, #{mem_id})
	</insert>
</mapper>